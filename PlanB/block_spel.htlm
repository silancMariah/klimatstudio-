        function checkLevelCompletion(level) {
            const levelData = gameState.levelData[level];
            const nextBtn = document.getElementById(`next-btn-${level}`);
            const finishBtn = document.getElementById('finish-btn');
            const finishBtn5 = document.getElementById('finish-btn-5');
            
            if (levelData.droppedBlocks.length === levelData.totalBlocks) {
                let isCorrectOrder = false;
                
                if (level === 2) {
                    // For level 2 (properties), order doesn't matter - just check if all blocks are placed
                    isCorrectOrder = true;
                } else {
                    // For other levels, check exact order
                    const currentOrder = levelData.droppedBlocks.map(block => block.order);
                    isCorrectOrder = JSON.stringify(currentOrder) === JSON.stringify(levelData.correctOrder);
                }
                
                if (isCorrectOrder) {
                    if (level < 4) {
                        if (nextBtn) nextBtn.disabled = false;
                    } else if (level === 4) {
                        // Show complete code when level 4 is completed
                        showCompleteCode();
                        const nextBtn4 = document.getElementById('next-btn-4');
                        if (nextBtn4) nextBtn4.disabled = false;
                    } else if (level === 5) {
                        if (finishBtn5) finishBtn5.disabled = false;
                    }
                    
                    // Add celebration effect
                    celebrateLevelCompletion(level);
                } else {
                    // Show order hint
                    showLevelOrderHint(level);
                }
            } else {
                if (level < 4) {
                    if (nextBtn) nextBtn.disabled = true;
                } else if (level === 4) {
                    // Hide complete code if level 4 is not completed
                    hideCompleteCode();
                    const nextBtn4 = document.getElementById('next-btn-4');
                    if (nextBtn4) nextBtn4.disabled = true;
                } else if (level === 5) {
                    if (finishBtn5) finishBtn5.disabled = true;
                }
            }
        }

