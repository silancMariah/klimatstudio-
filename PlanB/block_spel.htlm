                temperature: 15,
                co2: 380,
                age: 4500000000,
                solarEnergy: 100
            };
            
            // Reset all levels
            for (let level = 1; level <= 5; level++) {
                resetLevel(level);
            }
            
            // Remove any completion messages
            document.querySelectorAll('.feedback-message').forEach(msg => msg.remove());
            
            // Randomize blocks again for level 5
            randomizeAllBlocksForLevel5();
            
            // Go back to first level
            goToStep(1);
            
            // Reset planet
            updatePlanetAppearance('planet-1');
        }

        function generateFinalCode() {
            // Sort all blocks by their global order
            const sortedBlocks = gameState.allBlocks.sort((a, b) => a.globalOrder - b.globalOrder);
            
            const finalCode = sortedBlocks.map(block => block.code).join('\n');
            
            document.getElementById('final-code').textContent = finalCode;
        }

        function showFullCode() {
            const codeElement = document.getElementById('final-code');
            if (codeElement.style.maxHeight === 'none') {
                codeElement.style.maxHeight = '400px';
            } else {
                codeElement.style.maxHeight = 'none';
            }
        }



        // Initialize the game
        document.addEventListener('DOMContentLoaded', function() {
            initGame();
            updatePlanetAppearance();
        });
    </script>
