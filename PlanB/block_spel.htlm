        function removeBlockFromLevel(index, level) {
            const levelData = gameState.levelData[level];
            const blockData = levelData.droppedBlocks[index];
            if (!blockData) return;

            // Remove from DOM
            blockData.element.remove();
            
            // Mark original block as available
            blockData.originalBlock.classList.remove('used');
            
            // Remove from level array
            levelData.droppedBlocks.splice(index, 1);
            
            // Remove from global array
            gameState.allBlocks = gameState.allBlocks.filter(block => 
                !(block.code === blockData.code && block.level === level)
            );
            
            // Update indices for remaining blocks
            levelData.droppedBlocks.forEach((block, i) => {
                block.element.dataset.index = i;
                const removeBtn = block.element.querySelector('.remove-btn');
                removeBtn.setAttribute('onclick', `removeBlockFromLevel(${i}, ${level})`);
            });

            // Check if drop zone is empty
            if (levelData.droppedBlocks.length === 0) {
                document.getElementById(`code-drop-zone-${level}`).classList.remove('has-blocks');
            }

            updateLevelProgress(level);
            checkLevelCompletion(level);
        }

        function updateLevelProgress(level) {
            const levelData = gameState.levelData[level];
            const progress = (levelData.droppedBlocks.length / levelData.totalBlocks) * 100;
            
            const progressElement = document.getElementById(`progress-${level}`);
            const blocksPlacedElement = document.getElementById(`blocks-placed-${level}`);
            const totalBlocksElement = document.getElementById(`total-blocks-${level}`);
            
            if (progressElement) progressElement.style.width = progress + '%';
            if (blocksPlacedElement) blocksPlacedElement.textContent = levelData.droppedBlocks.length;
            if (totalBlocksElement) totalBlocksElement.textContent = levelData.totalBlocks;
        }

